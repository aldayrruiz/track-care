# Editor configuration, see https://editorconfig.org
root = true

[*.ts, *.js, *.html, *.scss]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = tab
indent_size = 2
trim_trailing_whitespace = true
[*.yml]
indent_style = space

[*.md]
max_line_length = off
trim_trailing_whitespace = false
